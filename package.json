  "scripts": {
    "admin": "node ./node_modules/express-admin/app.js .",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:sf": "babel-node node.js",
    "repair:db": "rm ./data/database.sqlite && touch ./data/database.sqlite ",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build2": "cd frontend && npm i &&  cd .. && tsc -p tsconfig.build.json  && node cli prepare:app",


    "heroku-build": "node dist-copy-database.js",


    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",

    "seed:generate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -c seed -n ",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -c seed",
    "seed:revert": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert -c seed",
    "seeds": "node cli seeds",
    "format:sf": "prettier --write \"**/*.js\"",
    "start:dev:sf": "nodemon",


    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
    "build": "node folder-copy-database.js && rimraf dist && nest build",
    "start:prod": "node dist/src/main.js && node dist-copy-database.js"
  },

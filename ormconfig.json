{
  "type": "sqlite",
  "database": "./data/database.sqlite",
  "logging": true,
  "autoLoadEntities": true,
  "synchronize": true,
  "entities": [ "dist/**/*.entity.js" ],
  "migrations": [ "dist/**/*..js" ],
  "subscribers": [ "src/subscriber/**/*.ts" ],
  "cli": {
    "entitiesDir": "dist/src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}